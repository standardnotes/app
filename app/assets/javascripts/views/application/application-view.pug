  sessions-modal(
    application='self.application'
    app-state='self.appState'
  )
  preferences(
    app-state='self.appState'
    application='self.application'
  )
  challenge-modal(
    ng-repeat="challenge in self.challenges track by challenge.id"
    class="sk-modal"
    application="self.application"
    challenge="challenge"
    on-dismiss="self.removeChallenge(challenge)"
  )
  notes-context-menu(
    application='self.application'
    app-state='self.appState'
  )
  purchase-flow(
    application='self.application'
    app-state='self.appState'
  )
