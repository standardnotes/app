{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["lib"],
    "exclude": ["node_modules"]
  },
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "docdash": {
    "meta": {
      "title": "SNJS Documentation"
    }
  }
}
