{
  "env": {
    "node": true,
    "commonjs": true
  },
  "extends": ["@react-native-community", "plugin:react-hooks/recommended", "../../.eslintrc.json"],
  "rules": {
    "no-console": "warn",
    "@typescript-eslint/no-explicit-any": "warn",
